http = require 'http'

class Init
  constructor: (args) ->
    @options = args
    @app = http.createServer  (req, res) ->
      res.statusCode = 200
      res.end('hello world')
    @

    #ready cb表示初始化的callback函数
  ready: (cb)->
    cb new Error('mock err')

module.exports = (args) ->
  new Init args